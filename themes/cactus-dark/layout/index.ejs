<section id="about">
  <p>Compiler Engineer at Google. Node.js Monkey Patcher. ‚ú®üê¢üöÄ‚ú®</p>
  <p style="display: inline">
    <% if (theme.customize.social_links) { %>
      Find me on
      <ul id="socialinfo">
        <% for(var i in theme.customize.social_links) { %>
          <li><a class="icon" target="_blank" href="<%- url_for(theme.customize.social_links[i]) %>"><i class="fa fa-<%= i %>"></i></a></li>
        <% } %>
      </ul>.
    <% } %>
  </p>
</section>

<section id="blog">
  <span class="h1"><a href="<%- url_for("archives") %>">Blog</a></span>
  <ul class="post-list">
    <% if (theme.customize.show_all_posts) { %>
      <% var show_posts = page.posts.sort('date', 'desc') %>
    <% } else { %>
      <% var show_posts = site.posts.sort('date', 'desc').limit(theme.customize.post_count || 5) %>
    <% } %>
    <% show_posts.each(function(post, i){ %>
      <li class="post-item">
        <%- partial('_partial/post/date', { post: post, class_name: 'meta' }) %>
        <span><%- partial('_partial/post/title', { post: post, index: true, class_name: '' }) %></span>
      </li>
    <% }); %>
  </ul>
  <% if (theme.customize.show_all_posts) { %>
    <%- partial('_partial/pagination') %>
  <% } %>
</section>

<section>
  <span class="h1">Videos</span>
    
  <iframe width="560" height="315" src="https://www.youtube.com/embed/p-iiEDtpy6I" frameborder="0" gesture="media" allow="encrypted-media" allowfullscreen></iframe>
  <iframe width="560" height="315" src="https://www.youtube.com/embed/PsDqH_RKvyc" frameborder="0" gesture="media" allow="encrypted-media" allowfullscreen></iframe>
</section>

<% if (site.data.oss) { %>
<section id="oss">
  <span class="h1">Open Source Contributions</span>
  <ul class="project-list">
    <% for(var obj in site.data.oss){ %>
      <li class="project-item">
        <a href="<%= site.data.oss[obj].url %>"><%= site.data.oss[obj].name %></a>: <%= site.data.oss[obj].desc %>
      </li>
    <% } %>
  </ul>
</section>
<% } %>


<% if (site.data.projects) { %>
  <section id="projects">
    <span class="h1">Projects</span>
    <ul class="project-list">
      <% for(var obj in site.data.projects){ %>
        <li class="project-item">
          <a href="<%= site.data.projects[obj].url %>"><%= site.data.projects[obj].name %></a>: <%= site.data.projects[obj].desc %>
        </li>
      <% } %>
    </ul>
  </section>
  <% } %>
